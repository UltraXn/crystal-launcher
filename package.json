{
  "name": "crystaltides-app",
  "version": "1.0.0",
  "packageManager": "npm@11.6.4",
  "private": true,
  "workspaces": [
    "apps/*",
    "plugins/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "client": "npm run dev -w @crystaltides/client",
    "server": "npm run dev -w @crystaltides/server",
    "install:all": "npm install"
  },
  "overrides": {
    "glob": "11.1.0",
    "esbuild": "0.27.2",
    "cross-spawn": "7.0.6",
    "brace-expansion": "2.0.2",
    "tar": "7.5.2",
    "minimatch": "9.0.5",
    "**": {
      "glob": "11.1.0",
      "tar": "7.5.2",
      "cross-spawn": "7.0.6",
      "brace-expansion": "2.0.2",
      "minimatch": "9.0.5",
      "tinyglobby": {
        "glob": "11.1.0"
      }
    },
    "tinyglobby": {
      "glob": "11.1.0"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.0.0",
    "cross-spawn": "7.0.6",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "fast-glob": "^3.3.3",
    "glob": "11.1.0",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "micromatch": "^4.0.8",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "sucrase": "^3.35.1",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.7.2",
    "typescript-eslint": "^8.50.1",
    "vite": "^6.2.0",
    "vitest": "^3.0.10"
